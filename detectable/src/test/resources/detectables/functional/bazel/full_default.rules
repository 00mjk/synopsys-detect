[
  {
    "targetDependenciesQueryBazelCmdArguments": [
      "query",
      "filter(\"@.*:jar\", deps(${detect.bazel.target}))"
    ],
    "dependencyToBazelExternalIdTransforms": [
      {
        "searchRegex": "^@",
        "replacementString": ""
      },
      {
        "searchRegex": "//.*",
        "replacementString": ""
      },
      {
        "searchRegex": "^",
        "replacementString": "//external:"
      }
    ],
    "dependencyDetailsXmlQueryBazelCmdArguments": [
      "query",
      "kind(maven_jar, ${detect.bazel.target.dependency})",
      "--output",
      "xml"
    ],
    "xPathQuery": "/query/rule[@class\u003d\u0027maven_jar\u0027]/string[@name\u003d\u0027artifact\u0027]",
    "ruleElementValueAttrName": "value",
    "artifactStringSeparatorRegex": ":"
  }
]